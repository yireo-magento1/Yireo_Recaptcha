<config>
    <translate>
        <modules>
            <Yireo_Recaptcha>
                <files>
                    <default>Yireo_Recaptcha.csv</default>
                </files>
            </Yireo_Recaptcha>
        </modules>
    </translate>

    <acl>
        <resources>
            <admin>
                <children>
                    <system>
                        <children>
                            <config>
                                <children>
                                    <recaptcha translate="title" module="recaptcha">
                                        <title>Recaptcha Section</title>
                                    </recaptcha>
                                </children>
                            </config>
                        </children>
                    </system>
                </children>
            </admin>
        </resources>
    </acl>
</config>